{
  "rewrites": [
    {
      "source": "/en",
      "destination": "/browser/en/index.*.html"
    },
    {
      "source": "/zh",
      "destination": "/browser/zh/index.*.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/server/en/main.*.mjs"
    },
    {
      "source": "/zh/:path*",
      "destination": "/server/zh/main.*.mjs"
    },
    {
      "source": "/",
      "destination": "/browser/en/index.*.html"
    },
    {
      "source": "/:path*",
      "destination": "/server/en/main.*.mjs"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}